{
  "$schema": "https://raw.githubusercontent.com/jetify-com/devbox/0.10.7/.schema/devbox.schema.json",
  "packages": [
    "go@latest",
    "postgresql@latest",
    "github:a-h/templ",
    "air@latest",
    "tailwindcss@latest",
  ],
  "shell": {
    "scripts": {
      "build": [
          "make build",
      ],
      "run": [
        "make run"
      ],
      "test": [
        "make test"
      ],
      "watch": [
        "make watch",
      ],
     "cover": [
        "make coverage",
      ],
    },
  },
}
